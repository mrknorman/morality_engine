          "when": [
            { "op": "decisions_gt", "value": 0 },
            { "op": "last_decision_remaining_lt_secs", "value": 1.0 }
          ],
          "then": [
            { "kind": "dialogue", "id": "lab_1.a.pass_slow" },
            { "kind": "dialogue", "id": "lab_1.b.intro" },
            { "kind": "dilemma", "id": "lab_1.near_sighted_bandit" }
